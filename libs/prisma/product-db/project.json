{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma/product-db/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/prisma/product-db/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/prisma/product-db"],
      "options": {
        "jestConfig": "libs/prisma/product-db/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "docker-db": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "docker-compose -f docker-compose.db.yml up -d"
      }
    },
    "db-migrate-save": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma migrate dev"
      }
    },
    "db-migrate-deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma migrate deploy"
      }
    },
    "db-generate": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma generate"
      }
    },
    "db-push": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma db push"
      }
    },
    "db-pull": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma db pull"
      }
    },
    "db-studio": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "prisma studio"
      }
    },
    "seed": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/prisma/product-db",
        "command": "ts-node scripts/seed.dev.ts"
      }
    }
  },
  "tags": []
}
